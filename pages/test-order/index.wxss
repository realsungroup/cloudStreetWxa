/* pages/test-order/index.wxss */

.list-item {
  padding: 24rpx;
  border-bottom: 1px solid #dcdcdc;
}
.device-header{
  border-bottom: 1px solid #dcdcdc;
  padding-bottom: 12rpx;
  display: flex;
}
.device-name{
  margin-right: 12rpx;
}
.device-orders {
  display: flex;
  flex-wrap: wrap;
  padding: 12rpx 0;
}
.device-order {
  width: 138rpx;
  padding: 12rpx;
  border: 1px solid #dcdcdc;
  border-radius: 20rpx;
  text-align: center;
  margin-right: 8rpx;
  margin-bottom: 8rpx;
}